    "cs_code": "[{\"cond\":\"article[style]\",\"rm_attrib\":\"style\"}]",
    "new_site": 1
  },
  "Euromoney": {
    "domain": "euromoney.com",
    "allow_cookies": 1,
    "useragent": "googlebot"
  },
  "Haaretz English": {
    "domain": "haaretz.com",
    "allow_cookies": 1,
    "useragent": "facebookbot"
  },
  "Harper's Magazine": {
    "domain": "harpers.org",
    "block_regex": "\\/harpers\\.org\\/wp-content\\/themes\\/timber\\/static\\/js\\/modal.+\\.js"
