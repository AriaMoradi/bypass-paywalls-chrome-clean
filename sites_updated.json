{
  "Krautreporter.de": {
    "domain": "krautreporter.de",
    "cs_code": [{
        "cond": ".js-article-paywall",
        "rm_elem": 1,
        "elems": [{
            "cond": ".js-paywall-divider",
            "rm_elem": 1
          }, {
            "cond": "#steady-checkout",
            "rm_elem": 1
          }, {
            "cond": ".blurred",
            "rm_class": "blurred"
          }
        ]
      }
    ]
  },
  "NRC Handelsblad": {
    "domain": "nrc.nl",
    "remove_cookies_select_drop": ["counter"],
    "block_regex": "\\.nrc\\.nl\\/paywall-api\\/api\\/zephr"
  },
  "Scientific American (free articles only)": {
    "domain": "scientificamerican.com",
    "allow_cookies": 1,
    "block_regex": "\\.scientificamerican\\.com\\/core\\/modules\\/.+\\/core_assets\\/js\\/mura\\.min\\.js"
  },
  "The Japan Times": {
    "domain": "japantimes.co.jp",
    "block_regex": "cdn\\.cxense\\.com\\/",
    "remove_cookies_select_drop": ["xbc"]
  }
}
