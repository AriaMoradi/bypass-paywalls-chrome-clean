    "useragent": "googlebot",
    "amp_unhide": 1
  },
  "Handelszeitung.ch": {
    "allow_cookies": 1,
    "domain": "handelszeitung.ch",
    "useragent": "googlebot"
  },
  "Knack.be": {
    "domain": "knack.be",
    "allow_cookies": 1,
    "block_regex": "(\\.knack\\.be\\/js\\/responsive\\/rmg(Modal|Paywall)\\.js|\\.blueconic\\.net\\/)",
    "cs_code": [{
        "cond": "div[id*=\"wall-modal\"]",
        "rm_elem": 1,
        "elems": [{
            "cond": "div.article-body > p",
            "rm_elem": 1
          }, {
            "cond": "div.article-body > style",
            "rm_elem": 1
          }
        ]
      }
    ]
  },
  "Le Vif.be": {
    "domain": "levif.be",
    "allow_cookies": 1,
    "block_regex": "(\\.levif\\.be\\/js\\/responsive\\/rmg(Modal|Paywall)\\.js|\\.blueconic\\.net\\/)",
    "cs_code": [{
        "cond": "div[id*=\"wall-modal\"]",
        "rm_elem": 1,
        "elems": [{
            "cond": "div.article-body > p",
            "rm_elem": 1
          }, {
            "cond": "div.article-body > style",
            "rm_elem": 1
          }
        ]
      }
    ]
  },
  "NRC Handelsblad": {
    "domain": "nrc.nl",
