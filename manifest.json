{
	"background": {
		"scripts": ["background.js"]
	},
	"content_security_policy": "script-src 'self'; object-src 'self'",
	"browser_action": {
		"default_popup": "popup.html"
	},
	"description": "Bypass News Sites' Paywalls",
	"homepage_url": "https://github.com/magnolia1234/bypass-paywalls-chrome-clean/blob/master/README.md",
	"icons": {
		"128": "bypass.png"
	},
	"manifest_version": 2,
	"name": "Bypass Paywalls Clean",
	"update_url": "https://raw.githubusercontent.com/magnolia1234/bypass-paywalls-chrome-clean/master/updates.xml",
	"short_name": "Bypass Paywall",
	"options_ui": {
		"chrome_style": true,
		"page": "options.html"
	},
    "permissions": [ "cookies", "<all_urls>", "storage", "webRequest", "webRequestBlocking"],
	"version": "1.6.1.2"
}
